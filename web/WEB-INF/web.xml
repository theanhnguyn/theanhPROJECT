<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>controller.customer.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertController</servlet-name>
        <servlet-class>controller.customer.InsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditController</servlet-name>
        <servlet-class>controller.customer.EditController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteController</servlet-name>
        <servlet-class>controller.customer.DeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.auth.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParentInsertController</servlet-name>
        <servlet-class>controller.parent.ParentInsertController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParentEditController</servlet-name>
        <servlet-class>controller.parent.ParentEditController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParentDeleteController</servlet-name>
        <servlet-class>controller.parent.ParentDeleteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAscIDController</servlet-name>
        <servlet-class>controller.customer.ListAscIDController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAscDobController</servlet-name>
        <servlet-class>controller.customer.ListAscDobController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ParentListAscCID</servlet-name>
        <servlet-class>controller.parent.ParentListAscCID</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WelcomeController</servlet-name>
        <servlet-class>controller.customer.WelcomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestController</servlet-name>
        <servlet-class>controller.customer.RequestController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/customer/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertController</servlet-name>
        <url-pattern>/customer/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditController</servlet-name>
        <url-pattern>/customer/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteController</servlet-name>
        <url-pattern>/customer/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParentInsertController</servlet-name>
        <url-pattern>/parent/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParentEditController</servlet-name>
        <url-pattern>/parent/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParentDeleteController</servlet-name>
        <url-pattern>/parent/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAscIDController</servlet-name>
        <url-pattern>/customer/listID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAscDobController</servlet-name>
        <url-pattern>/customer/listDOB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ParentListAscCID</servlet-name>
        <url-pattern>/parent/listCID</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WelcomeController</servlet-name>
        <url-pattern>/customer/welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestController</servlet-name>
        <url-pattern>/customer/request</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            10000000
        </session-timeout>
    </session-config>
</web-app>
